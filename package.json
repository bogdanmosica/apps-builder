{
  "name": "apps-builder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format-and-lint": "biome check . --no-errors-on-unmatched",
    "format-and-lint:fix": "biome check . --write --no-errors-on-unmatched",
    "cleandep": "rm -rf node_modules && pnpm -r exec rm -rf node_modules"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.5.1",
    "turbo": "^2.5.0",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "ignoredBuiltDependencies": ["@vercel/speed-insights"],
    "onlyBuiltDependencies": ["@vercel/speed-insights"]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "react-hook-form": "^7.55.0"
  }
}
