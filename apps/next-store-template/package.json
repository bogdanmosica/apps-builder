{
  "$schema": "https://json.schemastore.org/package",
  "name": "next-store-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepare": "pnpm husky",
    "docker:build": "docker build -t yournextstore .",
    "docker:run": "docker run -d -p 3000:3000 yournextstore"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.3.12",
    "@hookform/resolvers": "^5.0.1",
    "@next/env": "15.3.0",
    "@next/mdx": "15.3.0",
    "@radix-ui/react-icons": "1.3.2",
    "@splinetool/react-spline": "4.0.0",
    "@stripe/react-stripe-js": "3.6.0",
    "@stripe/stripe-js": "^2.2.2",
    "@t3-oss/env-nextjs": "0.12.0",
    "@types/mdx": "2.0.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "@workspace/next-auth": "workspace:*",
    "@workspace/types": "workspace:*",
    "@workspace/ui": "workspace:*",
    "@workspace/utils": "workspace:*",
    "@workspace/validations": "workspace:*",
    "ai": "4.3.6",
    "babel-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "cmdk": "1.1.1",
    "commerce-kit": "0.0.40",
    "intl-messageformat": "10.7.16",
    "lucide-react": "^0.475.0",
    "next": "^15.3.0",
    "next-auth": "5.0.0-beta.25",
    "next-mdx-remote": "5.0.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "schema-dts": "1.1.5",
    "sonner": "^2.0.3",
    "stripe": "^14.11.0",
    "tailwindcss": "^4.0.8",
    "tailwindcss-motion": "1.1.0",
    "trieve-ts-sdk": "0.0.86",
    "vaul": "1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@workspace/typescript-config": "workspace:*",
    "husky": "9.1.7"
  },
  "engines": {
    "node": "^20.11.1 || ^22.0.0"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "lint-staged": {
    "*": [
      "pnpm biome check --write --staged --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
